# INIT
git init
git status
git add ./*
git config --global --edit
git commit -m "Initial commit"

# PUSH REMOTE
git credential-osxkeychain
git config --global credential.helper osxkeychain
git credential-osxkeychain get
git remote add origin https://github.com/dimarudik/rda.git
git push -u origin master

# how to make COMMIT + PUSH
git status
git add .
git commit -m "1"
git push -u origin master

# PULL to get remote changes ( option --rebase is needed if there are uncommited local changes )
#git pull https://github.com/dimarudik/rda.git
git pull origin master

#CLONE
git clone https://github.com/dimarudik/rda.git

# VIEW COMMITS
git log --pretty=format:"%h - %s - %cd" --graph


# how to create BRANCH
git branch
git branch flow
git checkout flow
git push -u origin flow

# how to clone remote BRANCH
git clone -b flow https://github.com/dimarudik/rda.git
git clone -b Sharding https://github.com/dimarudik/rest-jpa.git rest-jpa
